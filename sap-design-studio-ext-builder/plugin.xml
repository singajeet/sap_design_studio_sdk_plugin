<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="com.armin.sap.ds.builder.ID"
         name="SAP Design Studio Extension Project Builder"
         point="org.eclipse.core.resources.builders">
      <builder
            hasNature="true">
         <run
               class="com.armin.sap.ds.builder.DesignStudioProjectBuilder">
         </run>
      </builder>
   </extension>
   <extension
         id="com.armin.sap.ds.builder.nature.ID"
         name="SAP Design Studio Project Nature"
         point="org.eclipse.core.resources.natures">
      <runtime>
         <run
               class="com.armin.sap.ds.builder.DesignStudioProjectNature">               
         </run>
      </runtime>
      <builder
            id="com.armin.sap.ds.builder.ID">
      </builder>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <category
            name="SAP Design Studio Project Commands"
            id="com.armin.sap.ds.builder.project.commands.category">
      </category>
      <command
            name="Toggle Design Studio Project Nature"
            defaultHandler="com.armin.sap.ds.builder.commands.handlers.ToggleProjectNatureHandler"
            categoryId="com.armin.sap.ds.builder.project.commands.category"
            id="com.armin.sap.ds.builder.commands.toggle.nature">
      </command>
      <command
            categoryId="com.armin.sap.ds.builder.project.commands.category"
            defaultHandler="com.armin.sap.ds.builder.commands.handlers.OpenExtensionHandler"
            id="com.armin.sap.ds.builder.commands.extension.open"
            name="Open Extension">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.projectConfigure?after=additions">
         <command
               commandId="com.armin.sap.ds.builder.commands.toggle.nature"
               disabledIcon="images/project_16x16_disabled.png"
               icon="images/project_16x16.png"
               label="Toggle SAP Design Studio Nature"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate>
                     <adapt
                           type="org.eclipse.core.resources.IProject">
                        <test
                              value="com.armin.sap.ds.builder.nature.ID"
                              property="org.eclipse.core.resources.projectNature">
                        </test>
                     </adapt>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
      </menuContribution>
      <menuContribution
            locationURI="toolbar:org.eclipse.ui.main.toolbar?after=additions">
         <toolbar
               id="com.armin.sap.ds.builder.toolbars.base_toolbar">
            <command
                  commandId="com.armin.sap.ds.builder.commands.toggle.nature"
                  disabledIcon="images/project_16x16_disabled.png"
                  icon="images/project_16x16.png"
                  tooltip="Toggles SAP Design Studio Nature of an project">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="selection">
                     <count
                           value="1">
                     </count>
                     <iterate>
                        <adapt
                              type="org.eclipse.core.resources.IProject">
                           <test
                                 property="org.eclipse.core.resources.projectNature"
                                 value="com.armin.sap.ds.builder.nature.ID">
                           </test>
                        </adapt>
                     </iterate>
                  </with>
               </visibleWhen>
            </command>
            <separator
                  name="sap-design-studio-ext-builder.separator1">
            </separator>
            <command
                  commandId="com.armin.sap.ds.builder.commands.extension.open"
                  icon="images/open_extension_16x16.png"
                  label="Open Extensions"
                  style="push">
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="selection">
                     <count
                           value="1">
                     </count>
                     <iterate
                           ifEmpty="false">
                        <instanceof
                              value="com.armin.sap.ds.builder.navigator.tree.ExtensionCollectionNode">
                        </instanceof>
                     </iterate>
                  </with>
               </visibleWhen>
            </command>
         </toolbar>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="popup:com.armin.sap.ds.builder.project.navigator.view?after=additions">
         <menu
               label="New">
            <command
                  commandId="org.eclipse.ui.newWizard"
                  disabledIcon="images/extension_16x16_disabled.png"
                  icon="images/extension_16x16.png"
                  label="Extension"
                  style="push">
               <parameter
                     name="newWizardId"
                     value="com.armin.sap.ds.builder.wizards.new_extension_wizard">
               </parameter>
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="selection">
                     <count
                           value="1">
                     </count>
                     <iterate
                           ifEmpty="false">
                        <instanceof
                              value="com.armin.sap.ds.builder.navigator.tree.ExtensionCollectionNode">
                        </instanceof>
                     </iterate>
                  </with>
               </visibleWhen>
            </command>
            <command
                  commandId="org.eclipse.ui.newWizard"
                  disabledIcon="images/component_16x16_disabled.png"
                  icon="images/component_16x16.png"
                  label="Component"
                  style="push">
               <parameter
                     name="newWizardId"
                     value="com.armin.sap.ds.builder.wizards.new_component_wizard">
               </parameter>
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="selection">
                     <count
                           value="1">
                     </count>
                     <iterate
                           ifEmpty="false">
                        <instanceof
                              value="com.armin.sap.ds.builder.navigator.tree.GroupNode">
                        </instanceof>
                     </iterate>
                  </with>
               </visibleWhen>
            </command>
            <command
                  commandId="org.eclipse.ui.newWizard"
                  disabledIcon="images/group_16x16_disabled.png"
                  icon="images/group_16x16.png"
                  label="Group"
                  style="push">
               <parameter
                     name="newWizardId"
                     value="com.armin.sap.ds.builder.wizards.new_group_wizard">
               </parameter>
               <visibleWhen
                     checkEnabled="false">
                  <with
                        variable="selection">
                     <count
                           value="1">
                     </count>
                     <iterate
                           ifEmpty="false">
                        <instanceof
                              value="com.armin.sap.ds.builder.navigator.tree.ExtensionNode">
                        </instanceof>
                     </iterate>
                  </with>
               </visibleWhen>
            </command>
         </menu>
         <separator
               name="com.armin.sap.ds.builder.separator.new-open">
         </separator>
         <command
               commandId="com.armin.sap.ds.builder.commands.extension.open"
               icon="images/extension_16x16.png"
               disabledIcon="images/extension_16x16_disabled.png"
               label="Open Extensions"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate
                        ifEmpty="false">
                     <instanceof
                           value="com.armin.sap.ds.builder.navigator.tree.ExtensionCollectionNode">
                     </instanceof>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.ui.navigate.openResource"
               label="Open File..."
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.project.openProject"
               icon="images/project_16x16.png"
               disabledIcon="images/project_16x16_disabled.png"
               label="Open Project..."
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate
                        ifEmpty="false">
                     <adapt
                           type="org.eclipse.core.resources.IProject">
                        <test
                              property="org.eclipse.core.resources.projectNature"
                              value="com.armin.sap.ds.builder.nature.ID">
                        </test>
                     </adapt>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
         <separator
               name="sap-design-studio-ext-navigator.separator3">
         </separator>
         <command
               commandId="org.eclipse.ui.edit.cut"
               label="Cut"
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.edit.copy"
               label="Copy"
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.edit.paste"
               label="Paste"
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.edit.delete"
               label="Delete"
               style="push">
         </command>
         <separator
               name="sap-design-studio-ext-navigator.separator4">
         </separator>
         <command
               commandId="org.eclipse.ui.file.import"
               label="Import..."
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.file.export"
               label="Export..."
               style="push">
         </command>
         <separator
               name="sap-design-studio-ext-navigator.separator5">
         </separator>
         <command
               commandId="org.eclipse.ui.file.refresh"
               label="Refresh"
               style="push">
         </command>true
         <command
               commandId="org.eclipse.ui.file.close"
               label="Close"
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.project.closeProject"
               label="Close Project"
               style="push">
         </command>
         <command
               commandId="org.eclipse.ui.project.closeUnrelatedProjects"
               label="Close Unrelated Projects"
               style="push">
         </command>
         <separator
               name="sap-design-studio-ext-navigator.separator6">
         </separator>
         <command
               commandId="org.eclipse.ui.file.properties"
               label="Properties"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="activeWorkbenchWindow.activePerspective">
                  <equals
                        value="com.armin.sap.ds.builder.project.perspective">
                  </equals>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="com.armin.sap.ds.builder.commands.toggle.nature"
               disabledIcon="images/project_16x16_disabled.png"
               icon="images/project_16x16.png"
               label="Toggle SAP Design Studio Nature"
               style="push">
         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:file?after=additions">
         <command
               commandId="com.armin.sap.ds.builder.commands.extension.open"
               disabledIcon="images/extension_16x16_disabled.png"
               icon="images/extension_16x16.png"
               label="Open Extensions"
               style="push">
            <visibleWhen
                  checkEnabled="false">
               <with
                     variable="selection">
                  <count
                        value="1">
                  </count>
                  <iterate
                        ifEmpty="false">
                     <instanceof
                           value="com.armin.sap.ds.builder.navigator.tree.ExtensionCollectionNode">
                     </instanceof>
                  </iterate>
               </with>
            </visibleWhen>
         </command>
         <command
               commandId="org.eclipse.ui.file.refresh"
               label="Refresh"
               style="push">
         </command>
      </menuContribution>
   </extension>
   <extension
         id="com.armin.sap.ds.builder.xmlProblem"
         name="XML Problem"
         point="org.eclipse.core.resources.markers">
      <super
            type="org.eclipse.core.resources.problemmarker">
      </super>
      <persistent
            value="true">
      </persistent>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="com.armin.sap.ds.builder.view.category"
            class="org.eclipse.ui.navigator.CommonNavigator"
            icon="images/sap_18.png"
            id="com.armin.sap.ds.builder.project.navigator.view"
            name="SAP Design Studio Project Navigator"
            inject="true">
      </view>
      <category
            name="SAP Design Studio"
            id="com.armin.sap.ds.builder.view.category">
      </category>
      <view
            name="SAP Design Studio Default View"
            inject="true"
            icon="images/quickview-icon.png"
            category="com.armin.sap.ds.builder.view.category"
            class="com.armin.sap.ds.builder.views.DesignStudioDefaultView"
            id="com.armin.sap.ds.builder.views.default">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.viewer">
      <viewerContentBinding
            viewerId="com.armin.sap.ds.builder.project.navigator.view">
         <includes>
            <contentExtension
                  pattern="com.armin.sap.ds.builder.project.navigator.dscontent">
            </contentExtension>
            <actionExtension
                  pattern="com.armin.sap.ds.builder.project.navigator.dsaction">
            </actionExtension>            
         </includes>
      </viewerContentBinding>
      <viewerActionBinding
            viewerId="com.armin.sap.ds.builder.project.navigator.view">
         <includes>
            <actionExtension
                  pattern="org.eclipse.ui.navigator.resources.*">
            </actionExtension>
         </includes>
      </viewerActionBinding>
   </extension>
   <extension
         point="org.eclipse.ui.navigator.navigatorContent">
      <navigatorContent
            activeByDefault="true"
            contentProvider="com.armin.sap.ds.builder.navigator.providers.NavigatorContentProvider"
            icon="images/sap_18.png"
            id="com.armin.sap.ds.builder.project.navigator.dscontent"
            labelProvider="com.armin.sap.ds.builder.navigator.providers.NavigatorLabelProvider"
            priority="normal"
            name="Design Studio Navigator Content">         
         <triggerPoints>
            <or>
               <instanceof
                     value="org.eclipse.core.resources.IResource">
               </instanceof>
            	<instanceof
	                  value="com.armin.sap.ds.builder.navigator.tree.ProjectItemNode">
	            </instanceof>
            </or>	            
         </triggerPoints>
         <possibleChildren>
            <instanceof
                  value="com.armin.sap.ds.builder.navigator.tree.ProjectItemNode">
            </instanceof>
         </possibleChildren>
      </navigatorContent>
   </extension>
   <extension
         point="org.eclipse.ui.perspectiveExtensions">
      <perspectiveExtension
            targetID="com.armin.sap.ds.builder.project.perspective">
         <view
               id="com.armin.sap.ds.builder.project.navigator.view"
               minimized="false"
               relationship="left"
               relative="org.eclipse.ui.editorss"
               visible="true">
         </view>
      </perspectiveExtension>
   </extension>
   <extension
         point="org.eclipse.ui.decorators">
      <decorator
            adaptable="true"
            class="com.armin.sap.ds.builder.decorators.DesignStudioLabelDecorator"
            icon="icons/sample_decorator.gif"
            id="com.armin.sap.ds.builder.project.item.decorator"
            label="Resource Decorator"
            lightweight="true"
            location="TOP_LEFT"
            state="false">
         <enablement>
            <and>
               <objectClass
                     name="org.eclipse.core.resources.IResource">
               </objectClass>
               <or>
                  <objectClass
                        name="org.eclipse.core.resources.IProject">
                  </objectClass>
                  <objectClass
                        name="org.eclipse.core.resources.IFile">
                  </objectClass>
                  <objectClass
                        name="com.armin.sap.ds.builder.navigator.tree.IProjectItemNode">
                  </objectClass>
               </or>
            </and>
         </enablement>
      </decorator>
      <decorator
            icon="icons/sample_decorator.gif"
            id="com.armin.sap.ds.builder.label.decorator"
            label="Design Studio Label Decorator">
         <enablement>
            <and>
               <objectClass
                     name="org.eclipse.core.resources.IResource">
               </objectClass>
               <or>
                  <objectClass
                        name="org.eclipse.core.resources.IProject">
                  </objectClass>
                  <objectClass
                        name="org.eclipse.core.resources.IFile">
                  </objectClass>
                  <objectClass
                        name="com.armin.sap.ds.builder.navigator.tree.IProjectItemNode">
                  </objectClass>
               </or>
            </and>
         </enablement>
      </decorator>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.armin.sap.ds.builder.editors.ExtensionEditor"
            contributorClass="com.armin.sap.ds.builder.editors.ExtensionEditorContributor"
            default="true"
            extensions="xml"
            icon="images/extension_28x28.png"
            id="com.armin.sap.ds.builder.editors.extension_editor"
            name="SAP Design Studio Extension Editor">
