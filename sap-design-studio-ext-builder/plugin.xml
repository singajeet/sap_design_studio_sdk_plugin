            id="com.armin.sap.ds.builder.editors.generic_editor"
            name="SAP Design Studio Generic Editor">
      </editor>
      <editor
            class="com.armin.sap.ds.builder.editors.DesignStudioExtensionsEditor"
            id="com.armin.sap.ds.builder.editors.DesignStudioExtensionsEditor"
            name="Extension Editor">
      </editor>
      <editor
            class="com.armin.sap.ds.builder.editors.DSEditorMainPage"
            id="com.armin.sap.ds.builder.editors.DSEditorMainPage"
            name="Main Page">
      </editor>
      <editor
            class="com.armin.sap.ds.builder.editors.ExtEditor"
            icon="images/extension_16x16.png"
            id="com.armin.sap.ds.builder.editors.ExtEditor"
            name="Extension Editor">
      </editor>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
