            id="com.armin.sap.ds.builder.contenttype.extension"
            name="%content-type.name"
            priority="normal">
         <describer
               class="org.eclipse.core.runtime.content.XMLRootElementContentDescriber2">
            <parameter
                  name="element"
                  value="sdkExtension">
            </parameter>
         </describer>
      </content-type>
      <content-type
            base-type="org.eclipse.core.runtime.xml"
            file-extensions="xml"
            file-names="plugin.xml"
            id="com.armin.sap.ds.builder.contenttype.plugin"
            name="%content-type.name.0"
            priority="normal">
         <describer
               class="org.eclipse.core.runtime.content.XMLRootElementContentDescriber2">
            <parameter
                  name="element"
                  value="plugin">
            </parameter>
         </describer>
      </content-type>
