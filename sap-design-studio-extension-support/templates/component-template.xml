<component
			id="${className}"
			title="${title}"
			tooltip="${tooltip}"
			icon="${icon}"
			handlerType="${handlerType}"
			modes="${modes}"
			${databound}
			${propertySheetPath}
			${group}
			>
			
	<!-- requireJs should go here -->
	${requireJs}
	<!-- css include should go here -->
	${cssInclude}
	<!-- all property definitions will go here -->
	${properties}
	
	<!-- default values section -->
	<initialization>
		${defaultValues}
	</initialization>
</component>